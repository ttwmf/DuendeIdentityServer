@model List<WebApp.Models.Weather>

@{
    ViewBag.Title = "Weather";
    Layout = "_Layout";
}

<h2>weather</h2>

<table>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Date</td>
            <td>@item.Summary</td>
            <td>@item.TemperatureC</td>
            <td>@item.TemperatureF</td>
        </tr>
    }
</table>